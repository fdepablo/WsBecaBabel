<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="spiderman" class="entity.SuperHeroe">
		<property name="nombre" value="Peter Parker"></property>
		<property name="nombreSuperHeroe" value="Spiderman"></property>
		<property name="poder" value="70"></property>
		<property name="superPoder" value="Hombre araÃ±a"></property>
	</bean>
	
	<bean id="thor" class="entity.SuperHeroe">
		<property name="nombre" value="Thor"></property>
		<property name="nombreSuperHeroe" value="Thor"></property>
		<property name="poder" value="90"></property>
		<property name="superPoder" value="Dios del trueno"></property>
	</bean>
	
	<bean id="hulk" class="entity.SuperHeroe">
		<property name="nombre" value="Bruce Banner"></property>
		<property name="nombreSuperHeroe" value="Hulk"></property>
		<property name="poder" value="90"></property>
		<property name="superPoder" value="Super fuerza"></property>
	</bean>
	
	<bean id="heroePoder" class="entity.SuperHeroe" scope="prototype">
		<property name="poder" value="100"></property>
	</bean>
	
	<bean id="heroeSuperPoder" class="entity.SuperHeroe" scope="prototype">
		<property name="superPoder" value="volar"></property>
	</bean>
	
	<bean id="listaPoder" class="java.util.ArrayList"></bean>
	
	<bean id="listaSuperPoder" class="java.util.ArrayList"></bean>
	
	<bean id="numeroDeSuperHeroes" class="java.lang.Integer">
		<constructor-arg value="3"></constructor-arg>
	</bean>
</beans>